<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Empresa.
    -->
    <changeSet id="20200517195600-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="numero_documento_id"
                                 baseTableName="ct_empresa_tb"
                                 constraintName="fk_ct_empresa_tb_numero_documento_id"
